{{>sparqlPreamble}}

CONSTRUCT {
    ?annotation ?a ?b.
    ?target ?e ?f.
    ?selector ?g ?h.
} WHERE {
    ?annotation oa:hasBody ?body.
    ?annotation oa:hasTarget ?target;
                ?a ?b.
    ?target oa:hasSource <{{sourceURI}}>;
            oa:hasSelector ?selector;
            ?e ?f.
    ?selector ?g ?h;
              oa:start ?start.
}
