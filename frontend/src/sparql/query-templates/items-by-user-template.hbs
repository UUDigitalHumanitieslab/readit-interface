{{!-- {{>sparqlPreamble}} --}}
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX oa: <http://www.w3.org/ns/oa#>
CONSTRUCT {?node ?p ?o}
WHERE {
    ?node ?p ?o.
    {{#if userURI}}
    ?node dcterms:creator <{{userURI}}>.
    {{/if}}
    {{#if filterMax}}
    FILTER (STR(?node) >= "{{{filterMin}}}" && STR(?node) <= "{{{filterMax}}}")
    {{/if}}
    MINUS {
      ?node a oa:SpecificResource.
    }
  	MINUS {
      ?node a oa:Annotation.
    }
  	MINUS {
      ?node a oa:TextQuoteSelector.
    }
    MINUS {
      ?node a oa:TextPositionSelector.
    }
}
ORDER BY desc(?node)
