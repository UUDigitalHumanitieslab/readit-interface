<nav class="pagination" role="navigation" aria-label="pagination">
  <a class="btn pagination-previous">Previous</a>
  <a class="btn pagination-next">Next page</a>
  <ul class="pagination-list">
    {{#if showPaginationLinks.start}}
    <li>
      <a id="page-first" class="pagination-link" aria-label="Goto page 1">1</a>
    </li>
    {{/if}}
    {{#if showPaginationLinks.ellipsisStart}}
    <li>
      <span class="pagination-ellipsis">&hellip;</span>
    </li>
    {{/if}}
    <li>
      <a id="page-minus" class="pagination-link">{{pageMinus}}</a>
    </li>
    <li>
      <a id="page-center" class="pagination-link" aria-current="page">{{pageCenter}}</a>
    </li>
    {{#if showPaginationLinks.pagePlus}}
    <li>
      <a id="page-plus" class="pagination-link">{{pagePlus}}</a>
    </li>
    {{/if}}
    {{#if showPaginationLinks.ellipsisEnd}}
    <li>
      <span class="pagination-ellipsis">&hellip;</span>
    </li>
    {{/if}}
    {{#if showPaginationLinks.end}}
    <li>
      <a id="page-last" class="pagination-link" aria-label="Goto last page">{{totalPages}}</a>
    </li>
    {{/if}}
  </ul>
</nav>